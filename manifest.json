{
  "manifest_version": 2,
  "name": "cifra",
  "version": "1.0",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.cifraclub.com.br/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "id@psdauophuo3h973h",
      "strict_min_version": "88.0"
    }
  }
}